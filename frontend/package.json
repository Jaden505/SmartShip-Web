{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "npx http-server -p ${PORT} ./dist",
    "test:unit": "vue-cli-service test:unit",
    "test:clear-cache": "node_modules/.bin/jest --clearCache"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-brands-svg-icons": "^6.2.0",
    "@fortawesome/free-regular-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/vue-fontawesome": "^3.0.1",
    "@vueuse/core": "^9.9.0",
    "@vueuse/motion": "^2.0.0-beta.12",
    "axios": "^1.1.3",
    "chart.js": "^3.9.1",
    "click-outside-vue3": "^4.0.1",
    "core-js": "^3.8.3",
    "eslint-plugin-jest": "^27.2.1",
    "http_common": "^1.0.3",
    "http-server": "^14.1.1",
    "leaflet": "^1.9.3",
    "leaflet-ptv-developer": "^1.0.2",
    "libre": "^0.2.7",
    "maplibre-gl": "^2.4.0",
    "vee-validate": "^4.7.1",
    "vue": "^3.2.13",
    "vue-chartjs": "^4.1.2",
    "vue-router": "^4.1.5",
    "vue-toastification": "^2.0.0-rc.5",
    "vuex": "^4.1.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "^5.0.8",
    "@vue/cli-service": "~5.0.0",
    "@vue/test-utils": "^2.0.2",
    "@vue/vue3-jest": "^27.0.0-alpha.4",
    "autoprefixer": "^10.4.13",
    "babel-jest": "^27.5.1",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "jest": "^27.5.1",
    "jest-expect-message": "^1.1.2",
    "jest-localstorage-mock": "^2.4.26",
    "postcss": "^8.4.18",
    "tailwindcss": "^3.2.1"
  },
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest",
    "setupFilesAfterEnv": [
      "jest-expect-message",
      "jest-localstorage-mock"
    ],
    "testMatch": [
      "**/src/**/*.spec.js"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "/images/(.*)$": "<rootDir>/public/images/$1",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "axios": "axios/dist/node/axios.cjs"
    },
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.vue$": "@vue/vue3-jest",
      "^.+\\.(jpg|jpeg|png|gif|svg|mp3|mp4|mpeg)$": "<rootDir>/jest-media-transformer.js"
    },
    "verbose": false
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "jest": true
    },
    "plugins": [
      "jest"
    ],
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "vue/multi-word-component-names": "off",
      "vue/no-mutating-props": "off",
      "no-unused-vars": "off",
      "no-unreachable": "off",
      "jest/valid-expect": [
        "error",
        {
          "maxArgs": 2
        }
      ]
    },
    "overrides": [
      {
        "files": [
          "**/src/**/*.spec.js"
        ],
        "plugins": [
          "jest"
        ],
        "extends": [
          "plugin:jest/recommended"
        ]
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
